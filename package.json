{
  "name": "benchmark-protocol",
  "version": "0.0.1",
  "description": "Core smart contracts of Benchmark Protocol.",
  "author": "Benchmark Protocol Team",
  "license": "MIT",
  "devDependencies": {
    "@nomiclabs/hardhat-etherscan": "^2.0.0",
    "@nomiclabs/hardhat-truffle5": "^2.0.0",
    "@nomiclabs/hardhat-web3": "^2.0.0",
    "@openzeppelin/contracts": "^3.2.1-solc-0.7",
    "@openzeppelin/test-helpers": "0.5.7",
    "@tenderly/hardhat-tenderly": "^1.0.3",
    "@truffle/hdwallet-provider": "^1.1.1",
    "chai": "4.2.0",
    "chai-as-promised": "7.1.1",
    "chai-bn": "0.2.1",
    "cp": "^0.2.0",
    "cross-var": "^1.1.0",
    "dotenv": "^8.2.0",
    "eslint": "^7.12.1",
    "eslint-config-prettier": "^6.14.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-mocha": "^8.0.0",
    "eslint-plugin-prettier": "^3.1.4",
    "ethereum-waffle": "^3.2.1",
    "ganache-cli": "^6.12.1",
    "got": "^11.8.0",
    "hardhat": "^2.0.2",
    "mathjs": "7.5.1",
    "mocha": "^8.2.0",
    "openzeppelin-solidity": "2.3.0",
    "prettier": "2.1.2",
    "prettier-plugin-solidity": "1.0.0-alpha.59",
    "rlp": "^2.2.6",
    "solc": "0.7.4",
    "solhint": "3.3.0",
    "yargs": "16.1.0"
  },
  "directories": {
    "test": "test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ayobuenavista/benchmark-protocol.git"
  },
  "scripts": {
    "deploy:aave": "yarn hardhat run scripts/deploy_aave.js",
    "compile": "yarn hardhat compile --config hardhat.config.js && yarn size",
    "console:dev": "yarn hardhat console",
    "coverage": "yarn hardhat clean && yarn hardhat coverage --config ./hardhat.config.js --testfiles '' --temp ''",
    "ganache": "bash scripts/start_ganache.sh",
    "fork": "yarn hardhat node",
    "lint": "yarn solhint 'contracts/**/*.sol' && yarn prettier -c './**/*.js'",
    "size": "node scripts/contractBytecodeSize.js",
    "test": "yarn hardhat test",
    "waffle:build": "waffle",
    "waffle:test": "export NODE_ENV=test && mocha"
  }
}
