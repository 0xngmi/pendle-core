{
  "name": "benchmark-protocol",
  "version": "0.0.1",
  "description": "Core smart contracts of Benchmark Protocol.",
  "author": "Benchmark Protocol Team",
  "license": "MIT",
  "devDependencies": {
    "@nomiclabs/hardhat-ethers": "^2.0.1",
    "@nomiclabs/hardhat-etherscan": "^2.0.0",
    "@nomiclabs/hardhat-waffle": "^2.0.1",
    "@openzeppelin/contracts": "^3.2.1-solc-0.7",
    "@openzeppelin/test-helpers": "0.5.9",
    "@tenderly/hardhat-tenderly": "^1.0.3",
    "@typechain/ethers-v5": "^5.0.0",
    "@types/mocha": "^8.2.0",
    "chai": "4.2.0",
    "chai-as-promised": "7.1.1",
    "chai-bn": "0.2.1",
    "cp": "^0.2.0",
    "cross-var": "^1.1.0",
    "dotenv": "^8.2.0",
    "eslint": "^7.12.1",
    "eslint-config-prettier": "^6.14.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-mocha": "^8.0.0",
    "eslint-plugin-prettier": "^3.1.4",
    "ethereum-waffle": "^3.2.1",
    "ethers": "^5.0.0",
    "ganache-cli": "^6.12.1",
    "got": "^11.8.0",
    "hardhat": "^2.0.5",
    "hardhat-typechain": "^0.3.3",
    "mathjs": "7.5.1",
    "mocha": "^8.2.0",
    "openzeppelin-solidity": "3.3.0",
    "prettier": "2.1.2",
    "prettier-plugin-solidity": "1.0.0-beta.2",
    "rlp": "^2.2.6",
    "solc": "0.7.5",
    "solhint": "3.3.0",
    "solidity-coverage": "^0.7.13",
    "ts-generator": "^0.1.1",
    "ts-node": "^9.1.1",
    "typechain": "^4.0.1",
    "typescript": "^4.1.2",
    "yargs": "16.2.0"
  },
  "directories": {
    "test": "test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ayobuenavista/benchmark-protocol.git"
  },
  "scripts": {
    "deploy:aave": "yarn hardhat run scripts/deploy_aave.js",
    "compile": "yarn hardhat compile && yarn size",
    "console:dev": "yarn hardhat console",
    "coverage": "yarn hardhat clean && yarn hardhat coverage --config ./hardhat.config.ts --testfiles '' --temp ''",
    "ganache": "bash scripts/start_ganache.sh",
    "fork": "yarn hardhat node",
    "lint": "yarn solhint 'contracts/**/*.sol' && yarn prettier -c './**/*.js'",
    "size": "yarn ts-node scripts/contractBytecodeSize.js",
    "test": "yarn hardhat test --network hardhat \"test/{core, libraries}/*.ts\"",
    "ci": "yarn hardhat compile && yarn hardhat coverage --network hardhat --testfiles \"test/{core, libraries}/*.ts\""
  }
}
